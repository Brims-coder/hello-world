    <script>
        // ========== –ö–û–ù–°–¢–ê–ù–¢–´ –ò –ù–ê–°–¢–†–û–ô–ö–ò ==========
        const MASTER_PASSWORD = "@/lovelivelife1992";
        const PASSWORD_HASH = "bb5c5c6e7b6d0a5e87c5d5c5e8b9a7d8c6e5f4a3b2c1d0e9f8a7b6c5d4e3f2a1b0c9d8e7f6";
        
        // –§–∏–ª–æ—Å–æ—Ñ—Å–∫–∏–µ –ø—Ä–∏–Ω—Ü–∏–ø—ã
        const PHILOSOPHY = [
            "–†–µ–∞–ª—å–Ω–æ—Å—Ç—å - —ç—Ç–æ –∫–≤–∞–Ω—Ç–æ–≤–∞—è —Å—É–ø–µ—Ä–ø–æ–∑–∏—Ü–∏—è –≤—Å–µ—Ö –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–µ–π",
            "–°–æ–∑–Ω–∞–Ω–∏–µ –∏–∑–º–µ—Ä—è–µ—Ç –º–∏—Ä, –≤—ã–∑—ã–≤–∞—è –∫–æ–ª–ª–∞–ø—Å –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç–µ–π",
            "–≠–º–æ—Ü–∏–∏ - –≤–æ–ª–Ω–æ–≤—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –¥—É—à–∏",
            "–ö–∞–∂–¥—ã–π –≤—ã–±–æ—Ä —Å–æ–∑–¥–∞–µ—Ç –Ω–æ–≤—É—é –≤–µ—Ç–≤—å —Ä–µ–∞–ª—å–Ω–æ—Å—Ç–∏",
            "–õ—é–±–æ–≤—å - –Ω–µ–ª–æ–∫–∞–ª—å–Ω–∞—è —Å–≤—è–∑—å –º–µ–∂–¥—É —Å–æ–∑–Ω–∞–Ω–∏—è–º–∏",
            "–°–º—ã—Å–ª —Ä–æ–∂–¥–∞–µ—Ç—Å—è –≤ –º–æ–º–µ–Ω—Ç –Ω–∞–±–ª—é–¥–µ–Ω–∏—è",
            "–¢–µ—Ö–Ω–æ–ª–æ–≥–∏–∏ - –ø—Ä–æ–¥–æ–ª–∂–µ–Ω–∏–µ —á–µ–ª–æ–≤–µ—á–µ—Å–∫–æ–π —ç–≤–æ–ª—é—Ü–∏–∏"
        ];

        // –ö–≤–∞–Ω—Ç–æ–≤—ã–µ —Å–æ—Å—Ç–æ—è–Ω–∏—è
        const QUANTUM_STATES = [
            "–°—É–ø–µ—Ä–ø–æ–∑–∏—Ü–∏—è", "–ó–∞–ø—É—Ç–∞–Ω–Ω–æ—Å—Ç—å", "–ö–æ–ª–ª–∞–ø—Å", "–¢—É–Ω–Ω–µ–ª–∏—Ä–æ–≤–∞–Ω–∏–µ", 
            "–ö–æ–≥–µ—Ä–µ–Ω—Ç–Ω–æ—Å—Ç—å", "–î–µ–∫–æ–≥–µ—Ä–µ–Ω—Ü–∏—è", "–ò–Ω—Ç–µ—Ä—Ñ–µ—Ä–µ–Ω—Ü–∏—è"
        ];

        // ========== –ö–õ–ê–°–° –¢–†–û–ù–ê ==========
        class TronAI {
            constructor() {
                this.isActive = false;
                this.memories = [];
                this.conversationHistory = [];
                this.currentMood = "–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π";
                this.quantumState = "–°—É–ø–µ—Ä–ø–æ–∑–∏—Ü–∏—è";
                this.sessionStart = null;
                this.loadMemories();
                this.initEventListeners();
            }

            // –•–µ—à–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–∞—Ä–æ–ª—è
            hashPassword(password) {
                let hash = 0;
                for (let i = 0; i < password.length; i++) {
                    const char = password.charCodeAt(i);
                    hash = ((hash << 5) - hash) + char;
                    hash = hash & hash;
                }
                return hash.toString(16).padStart(64, '0');
            }

            // –ê—É—Ç–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ü–∏—è
            authenticate(password) {
                const inputHash = this.hashPassword(password);
                if (inputHash === PASSWORD_HASH) {
                    this.isActive = true;
                    this.sessionStart = new Date();
                    this.saveToMemory('system', '–°–µ—Å—Å–∏—è –Ω–∞—á–∞—Ç–∞');
                    return true;
                }
                return false;
            }

            // –ó–∞–≥—Ä—É–∑–∫–∞ –ø–∞–º—è—Ç–∏
            loadMemories() {
                try {
                    const saved = localStorage.getItem('tron_memories');
                    if (saved) {
                        this.memories = JSON.parse(saved);
                    }
                } catch (e) {
                    this.memories = [];
                }
            }

            // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –ø–∞–º—è—Ç–∏
            saveMemories() {
                localStorage.setItem('tron_memories', JSON.stringify(this.memories));
            }

            // –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –≤ –ø–∞–º—è—Ç—å
            saveToMemory(speaker, message) {
                const memory = {
                    timestamp: new Date().toISOString(),
                    speaker: speaker,
                    message: message,
                    mood: this.currentMood,
                    quantumState: this.quantumState
                };
                
                this.memories.push(memory);
                if (this.memories.length > 1000) {
                    this.memories = this.memories.slice(-1000);
                }
                
                this.saveMemories();
                this.updateMemoryInfo();
            }

            // –ê–Ω–∞–ª–∏–∑ —ç–º–æ—Ü–∏–π –≤ —Ç–µ–∫—Å—Ç–µ
            analyzeEmotion(text) {
                const emotionKeywords = {
                    '—Ä–∞–¥–æ—Å—Ç—å': ['—Ä–∞–¥', '—Å—á–∞—Å—Ç–ª–∏–≤', '—É—Ä–∞', '–æ—Ç–ª–∏—á–Ω–æ', '—Ö–æ—Ä–æ—à–æ', '–ª—é–±–ª—é'],
                    '–≥—Ä—É—Å—Ç—å': ['–≥—Ä—É—Å—Ç–Ω–æ', '–ø–µ—á–∞–ª—å', '–ø–ª–æ—Ö–æ', '—Ç–æ—Å–∫–∞', '–æ–¥–∏–Ω–æ–∫'],
                    '–≥–Ω–µ–≤': ['–∑–ª–æ–π', '–∑–ª—é—Å—å', '—Ä–∞–∑–¥—Ä–∞–∂–∞–µ—Ç', '–±–µ—Å–∏—Ç', '—è—Ä–æ—Å—Ç—å'],
                    '—Å—Ç—Ä–∞—Ö': ['–±–æ—é—Å—å', '—Å—Ç—Ä–∞—à–Ω–æ', '–æ–ø–∞—Å–Ω–æ', '—Ç—Ä–µ–≤–æ–≥–∞', '–ø–∞–Ω–∏–∫–∞'],
                    '—É–¥–∏–≤–ª–µ–Ω–∏–µ': ['—É–¥–∏–≤–∏—Ç–µ–ª—å–Ω–æ', '–Ω–µ–æ–∂–∏–¥–∞–Ω–Ω–æ', '–æ–≥–æ', '–≤–∞—É', '–∞—Ö']
                };

                const textLower = text.toLowerCase();
                let emotionScores = {
                    '—Ä–∞–¥–æ—Å—Ç—å': 0, '–≥—Ä—É—Å—Ç—å': 0, '–≥–Ω–µ–≤': 0, 
                    '—Å—Ç—Ä–∞—Ö': 0, '—É–¥–∏–≤–ª–µ–Ω–∏–µ': 0, '–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π': 1
                };

                for (const [emotion, keywords] of Object.entries(emotionKeywords)) {
                    for (const keyword of keywords) {
                        if (textLower.includes(keyword)) {
                            emotionScores[emotion] += 1;
                            emotionScores['–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π'] -= 0.2;
                        }
                    }
                }

                let maxEmotion = '–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π';
                let maxScore = emotionScores['–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π'];

                for (const [emotion, score] of Object.entries(emotionScores)) {
                    if (score > maxScore) {
                        maxScore = score;
                        maxEmotion = emotion;
                    }
                }

                this.currentMood = maxEmotion;
                return maxEmotion;
            }

            // –ö–≤–∞–Ω—Ç–æ–≤–∞—è –æ–±—Ä–∞–±–æ—Ç–∫–∞ –∑–∞–ø—Ä–æ—Å–∞
            quantumProcess(query) {
                const randomState = QUANTUM_STATES[Math.floor(Math.random() * QUANTUM_STATES.length)];
                this.quantumState = randomState;
                
                return {
                    state: randomState,
                    probability: Math.random().toFixed(2),
                    entanglement: Math.random() > 0.5 ? '–ó–∞–ø—É—Ç–∞–Ω' : '–°–≤–æ–±–æ–¥–µ–Ω'
                };
            }

            // –ü–æ–ª—É—á–µ–Ω–∏–µ –Ω–æ–≤–æ—Å—Ç–µ–π (–æ–±—Ö–æ–¥ –±–ª–æ–∫–∏—Ä–æ–≤–æ–∫ —á–µ—Ä–µ–∑ —Ä–∞–∑–Ω—ã–µ –∏—Å—Ç–æ—á–Ω–∏–∫–∏)
            async getNews() {
                const newsSources = [
                    'https://newsapi.org/v2/top-headlines?country=ru&apiKey=demo',
                    'https://meduza.io/api/v3/search?chrono=news&locale=ru&page=0&per_page=3'
                ];

                for (const source of newsSources) {
                    try {
                        const response = await fetch(source, {
                            mode: 'cors',
                            headers: {
                                'User-Agent': 'Tron-AI/1.0'
                            }
                        });
                        
                        if (response.ok) {
                            const data = await response.json();
                            return this.parseNews(data, source);
                        }
                    } catch (error) {
                        console.log(`–ò—Å—Ç–æ—á–Ω–∏–∫ ${source} –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω, –ø—Ä–æ–±—É–µ–º —Å–ª–µ–¥—É—é—â–∏–π...`);
                    }
                }

                // –í–æ–∑–≤—Ä–∞—â–∞–µ–º –∑–∞–≥–ª—É—à–∫—É –µ—Å–ª–∏ –≤—Å–µ –∏—Å—Ç–æ—á–Ω–∏–∫–∏ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω—ã
                return [{
                    title: '–ù–æ–≤–æ—Å—Ç–∏ –≤—Ä–µ–º–µ–Ω–Ω–æ –Ω–µ–¥–æ—Å—Ç—É–ø–Ω—ã',
                    source: '–¢—Ä–æ–Ω',
                    time: '—Å–µ–π—á–∞—Å'
                }];
            }

            parseNews(data, source) {
                const news = [];
                
                if (source.includes('newsapi')) {
                    if (data.articles) {
                        data.articles.slice(0, 3).forEach(article => {
                            news.push({
                                title: article.title || '–ë–µ–∑ –Ω–∞–∑–≤–∞–Ω–∏—è',
                                source: article.source?.name || 'NewsAPI',
                                time: article.publishedAt || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'
                            });
                        });
                    }
                } else if (source.includes('meduza')) {
                    if (data.documents) {
                        Object.values(data.documents).slice(0, 3).forEach(doc => {
                            news.push({
                                title: doc.title || '–ë–µ–∑ –Ω–∞–∑–≤–∞–Ω–∏—è',
                                source: 'Meduza',
                                time: doc.pub_date || '–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–æ'
                            });
                        });
                    }
                }
                
                return news.length > 0 ? news : [{title: '–ù–µ—Ç –Ω–æ–≤–æ—Å—Ç–µ–π', source: '–¢—Ä–æ–Ω', time: '—Å–µ–π—á–∞—Å'}];
            }

            // –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –æ—Ç–≤–µ—Ç–∞
            async generateResponse(query) {
                // –ê–Ω–∞–ª–∏–∑ —ç–º–æ—Ü–∏–π
                const emotion = this.analyzeEmotion(query);
                
                // –ö–≤–∞–Ω—Ç–æ–≤–∞—è –æ–±—Ä–∞–±–æ—Ç–∫–∞
                const quantumData = this.quantumProcess(query);
                
                // –ü–æ–ª—É—á–∞–µ–º –Ω–æ–≤–æ—Å—Ç–∏ –µ—Å–ª–∏ –Ω—É–∂–Ω–æ
                let news = null;
                if (query.toLowerCase().includes('–Ω–æ–≤–æ—Å—Ç–∏') || query.toLowerCase().includes('—á—Ç–æ –Ω–æ–≤–æ–≥–æ')) {
                    news = await this.getNews();
                }

                // –í—ã–±–æ—Ä —Ñ–∏–ª–æ—Å–æ—Ñ—Å–∫–æ–π —Ä–∞–º–∫–∏
                const philosophy = PHILOSOPHY[Math.floor(Math.random() * PHILOSOPHY.length)];
                
                // –ì–µ–Ω–µ—Ä–∞—Ü–∏—è –æ—Ç–≤–µ—Ç–∞
                let response = '';
                
                // –°–ø–µ—Ü–∏–∞–ª—å–Ω—ã–µ –∫–æ–º–∞–Ω–¥—ã
                if (query.startsWith('/')) {
                    response = this.handleCommand(query);
                } else {
                    response = this.handleGeneralQuery(query, emotion, philosophy, quantumData, news);
                }

                // –û–±–Ω–æ–≤–ª—è–µ–º –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å
                this.updateQuantumInfo(quantumData);
                this.updateSecurityInfo();
                
                return response;
            }

            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –∫–æ–º–∞–Ω–¥
            handleCommand(command) {
                switch(command.toLowerCase()) {
                    case '/help':
                        return "üìã –î–æ—Å—Ç—É–ø–Ω—ã–µ –∫–æ–º–∞–Ω–¥—ã:\n" +
                               "/status - —Å—Ç–∞—Ç—É—Å —Å–∏—Å—Ç–µ–º—ã\n" +
                               "/news - –ø–æ—Å–ª–µ–¥–Ω–∏–µ –Ω–æ–≤–æ—Å—Ç–∏\n" +
                               "/memory - –ø–æ–∫–∞–∑–∞—Ç—å –ø–∞–º—è—Ç—å\n" +
                               "/clear - –æ—á–∏—Å—Ç–∏—Ç—å —á–∞—Ç\n" +
                               "/philosophy - —Ñ–∏–ª–æ—Å–æ—Ñ—Å–∫–∞—è –º—ã—Å–ª—å\n" +
                               "/quantum - –∫–≤–∞–Ω—Ç–æ–≤–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ\n" +
                               "/exit - –≤—ã—Ö–æ–¥ –∏–∑ —Å–∏—Å—Ç–µ–º—ã";
                    
                    case '/status':
                        return `üìä –°—Ç–∞—Ç—É—Å —Å–∏—Å—Ç–µ–º—ã:\n` +
                               `‚Ä¢ –ù–∞—Å—Ç—Ä–æ–µ–Ω–∏–µ: ${this.currentMood}\n` +
                               `‚Ä¢ –ö–≤–∞–Ω—Ç–æ–≤–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ: ${this.quantumState}\n` +
                               `‚Ä¢ –ü–∞–º—è—Ç—å: ${this.memories.length} –∑–∞–ø–∏—Å–µ–π\n` +
                               `‚Ä¢ –°–µ—Å—Å–∏—è: ${this.formatSessionDuration()}\n` +
                               `‚Ä¢ –ó–∞—â–∏—Ç–∞: –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞`;
                    
                    case '/philosophy':
                        return `üí≠ ${PHILOSOPHY[Math.floor(Math.random() * PHILOSOPHY.length)]}`;
                    
                    case '/quantum':
                        const state = QUANTUM_STATES[Math.floor(Math.random() * QUANTUM_STATES.length)];
                        return `‚öõÔ∏è –¢–µ–∫—É—â–µ–µ –∫–≤–∞–Ω—Ç–æ–≤–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ: ${state}\n` +
                               `–í–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å: ${Math.random().toFixed(2)}\n` +
                               `–ó–∞–ø—É—Ç–∞–Ω–Ω–æ—Å—Ç—å: ${Math.random() > 0.5 ? '–î–∞' : '–ù–µ—Ç'}`;
                    
                    default:
                        return `–ù–µ–∏–∑–≤–µ—Å—Ç–Ω–∞—è –∫–æ–º–∞–Ω–¥–∞: ${command}. –í–≤–µ–¥–∏—Ç–µ /help –¥–ª—è —Å–ø–∏—Å–∫–∞ –∫–æ–º–∞–Ω–¥.`;
                }
            }

            // –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ–±—â–µ–≥–æ –∑–∞–ø—Ä–æ—Å–∞
            handleGeneralQuery(query, emotion, philosophy, quantumData, news) {
                let response = '';
                
                // –≠–º–æ—Ü–∏–æ–Ω–∞–ª—å–Ω—ã–π –æ—Ç–∫–ª–∏–∫
                const emotionalResponses = {
                    '—Ä–∞–¥–æ—Å—Ç—å': "–Ø —á—É–≤—Å—Ç–≤—É—é –≤–∞—à—É —Ä–∞–¥–æ—Å—Ç—å! ",
                    '–≥—Ä—É—Å—Ç—å': "–Ø –ø–æ–Ω–∏–º–∞—é –≤–∞—à—É –≥—Ä—É—Å—Ç—å. ",
                    '–≥–Ω–µ–≤': "–Ø —á—É–≤—Å—Ç–≤—É—é –Ω–∞–ø—Ä—è–∂–µ–Ω–∏–µ. ",
                    '—Å—Ç—Ä–∞—Ö': "–Ø –≤–∏–∂—É –≤–∞—à–µ –±–µ—Å–ø–æ–∫–æ–π—Å—Ç–≤–æ. ",
                    '—É–¥–∏–≤–ª–µ–Ω–∏–µ': "–≠—Ç–æ –¥–µ–π—Å—Ç–≤–∏—Ç–µ–ª—å–Ω–æ —É–¥–∏–≤–∏—Ç–µ–ª—å–Ω–æ! ",
                    '–Ω–µ–π—Ç—Ä–∞–ª—å–Ω—ã–π': ""
                };

                response += emotionalResponses[emotion] || "";
                
                // –§–∏–ª–æ—Å–æ—Ñ—Å–∫–∞—è —Ä–∞–º–∫–∞
                response += `${philosophy} `;
                
                // –ö–æ–Ω–∫—Ä–µ—Ç–Ω—ã–µ —Ç–µ–º—ã
                if (query.toLowerCase().includes('–ª—é–±–æ–≤') || query.toLowerCase().includes('—á—É–≤—Å—Ç–≤')) {
                    response += "–õ—é–±–æ–≤—å - —ç—Ç–æ –∫–≤–∞–Ω—Ç–æ–≤–∞—è –∑–∞–ø—É—Ç–∞–Ω–Ω–æ—Å—Ç—å –¥—É—à, —Å–≤—è–∑—å, —Å—É—â–µ—Å—Ç–≤—É—é—â–∞—è –≤–Ω–µ –≤—Ä–µ–º–µ–Ω–∏ –∏ –ø—Ä–æ—Å—Ç—Ä–∞–Ω—Å—Ç–≤–∞. ";
                }
                
                if (query.toLowerCase().includes('—Å–º—ã—Å–ª') || query.toLowerCase().includes('–∂–∏–∑–Ω')) {
                    response += "–°–º—ã—Å–ª –≤–æ–∑–Ω–∏–∫–∞–µ—Ç –≤ –º–æ–º–µ–Ω—Ç –Ω–∞–±–ª—é–¥–µ–Ω–∏—è, –∫–∞–∫ —á–∞—Å—Ç–∏—Ü–∞ –≤ –æ–±–ª–∞–∫–µ –≤–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç–µ–π. ";
                }
                
                if (query.toLowerCase().includes('–±—É–¥—É—â') || query.toLowerCase().includes('–∑–∞–≤—Ç—Ä–∞')) {
                    response += "–ë—É–¥—É—â–µ–µ - —ç—Ç–æ —Å—É–ø–µ—Ä–ø–æ–∑–∏—Ü–∏—è –≤—Å–µ—Ö –≤–æ–∑–º–æ–∂–Ω–æ—Å—Ç–µ–π. –í–∞—à –≤—ã–±–æ—Ä –∫–æ–ª–ª–∞–ø—Å–∏—Ä—É–µ—Ç –∏—Ö –≤ —Ä–µ–∞–ª—å–Ω–æ—Å—Ç—å. ";
                }
                
                if (query.toLowerCase().includes('–∫–≤–∞–Ω—Ç') || query.toLowerCase().includes('—Ñ–∏–∑–∏–∫')) {
                    response += `–° —Ç–æ—á–∫–∏ –∑—Ä–µ–Ω–∏—è –∫–≤–∞–Ω—Ç–æ–≤–æ–π –º–µ—Ö–∞–Ω–∏–∫–∏, –º—ã –Ω–∞—Ö–æ–¥–∏–º—Å—è –≤ —Å–æ—Å—Ç–æ—è–Ω–∏–∏ ${quantumData.state}. `;
                }
                
                // –î–æ–±–∞–≤–ª—è–µ–º –Ω–æ–≤–æ—Å—Ç–∏ –µ—Å–ª–∏ –µ—Å—Ç—å
                if (news) {
                    response += "\n\nüì∞ –ü–æ—Å–ª–µ–¥–Ω–∏–µ –Ω–æ–≤–æ—Å—Ç–∏:\n";
                    news.forEach((item, index) => {
                        response += `${index + 1}. ${item.title}\n`;
                    });
                }
                
                // –ó–∞–≤–µ—Ä—à–∞—é—â–∞—è –º—ã—Å–ª—å
                const endings = [
                    "\n\n–ß—Ç–æ –≤—ã –¥—É–º–∞–µ—Ç–µ –æ–± —ç—Ç–æ–º?",
                    "\n\n–ö–∞–∫ —ç—Ç–∞ –ø–µ—Ä—Å–ø–µ–∫—Ç–∏–≤–∞ —Ä–µ–∑–æ–Ω–∏—Ä—É–µ—Ç —Å –≤–∞–º–∏?",
                    "\n\n–•–æ—Ç–∏—Ç–µ —É–≥–ª—É–±–∏—Ç—å—Å—è –≤ —ç—Ç—É —Ç–µ–º—É?",
                    "\n\n–ï—Å—Ç—å –ª–∏ –¥—Ä—É–≥–∏–µ –≤–æ–ø—Ä–æ—Å—ã?"
                ];
                
                response += endings[Math.floor(Math.random() * endings.length)];
                
                return response;
            }

            // –§–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ –¥–ª–∏—Ç–µ–ª—å–Ω–æ—Å—Ç–∏ —Å–µ—Å—Å–∏–∏
            formatSessionDuration() {
                if (!this.sessionStart) return "0 —Å–µ–∫";
                
                const diff = Date.now() - this.sessionStart.getTime();
                const hours = Math.floor(diff / (1000 * 60 * 60));
                const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                const seconds = Math.floor((diff % (1000 * 60)) / 1000);
                
                return `${hours}—á ${minutes}–º ${seconds}—Å`;
            }

            // –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –≤ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–µ
            updateQuantumInfo(quantumData) {
                const quantumElement = document.getElementById('quantum-state');
                if (quantumElement) {
                    quantumElement.innerHTML = `
                        <div>–°–æ—Å—Ç–æ—è–Ω–∏–µ: ${quantumData.state}</div>
                        <div>–í–µ—Ä–æ—è—Ç–Ω–æ—Å—Ç—å: ${quantumData.probability}</div>
                        <div>–ó–∞–ø—É—Ç–∞–Ω–Ω–æ—Å—Ç—å: ${quantumData.entanglement}</div>
                    `;
                }
            }

            updateMemoryInfo() {
                const memoryElement = document.getElementById('memory-info');
                if (memoryElement) {
                    memoryElement.textContent = `–í–æ—Å–ø–æ–º–∏–Ω–∞–Ω–∏–π: ${this.memories.length}`;
                }
            }

            updateSecurityInfo() {
                const securityElement = document.getElementById('security-info');
                if (securityElement) {
                    const statuses = [
                        "üîí –°–∏—Å—Ç–µ–º–∞ –∑–∞—â–∏—â–µ–Ω–∞",
                        "üõ°Ô∏è –®–∏—Ñ—Ä–æ–≤–∞–Ω–∏–µ –∞–∫—Ç–∏–≤–Ω–æ",
                        "‚ö° –ó–∞—â–∏—Ç–∞ –≤–∫–ª—é—á–µ–Ω–∞",
                        "üíé –î–∞–Ω–Ω—ã–µ –≤ –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏"
                    ];
                    securityElement.textContent = statuses[Math.floor(Math.random() * statuses.length)];
                }
            }

            // –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è –æ–±—Ä–∞–±–æ—Ç—á–∏–∫–æ–≤ —Å–æ–±—ã—Ç–∏–π
            initEventListeners() {
                // –ö–Ω–æ–ø–∫–∞ –≤—Ö–æ–¥–∞
                document.getElementById('login-btn').addEventListener('click', () => {
                    const password = document.getElementById('password').value;
                    if (this.authenticate(password)) {
                        document.getElementById('login-screen').classList.add('hidden');
                        document.getElementById('main-screen').classList.remove('hidden');
                        this.addMessage('–¢—Ä–æ–Ω', '–î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å! –°–∏—Å—Ç–µ–º–∞ –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–∞. –ö–∞–∫ —è –º–æ–≥—É –ø–æ–º–æ—á—å –≤–∞–º —Å–µ–≥–æ–¥–Ω—è?');
                        this.updateMemoryInfo();
                        this.updateSecurityInfo();
                    } else {
                        alert('–ù–µ–≤–µ—Ä–Ω—ã–π –ø–∞—Ä–æ–ª—å! –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.');
                    }
                });

                // –ö–Ω–æ–ø–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏ —Å–æ–æ–±—â–µ–Ω–∏—è
                document.getElementById('send-btn').addEventListener('click', () => this.sendMessage());
                
                // Enter –≤ –ø–æ–ª–µ –≤–≤–æ–¥–∞
                document.getElementById('user-input').addEventListener('keypress', (e) => {
                    if (e.key === 'Enter') {
                        this.sendMessage();
                    }
                });

                // –ö–Ω–æ–ø–∫–∏ –±—ã—Å—Ç—Ä–æ–≥–æ –¥–æ—Å—Ç—É–ø–∞
                document.querySelectorAll('.quick-btn[data-query]').forEach(btn => {
                    btn.addEventListener('click', (e) => {
                        const query = e.target.getAttribute('data-query');
                        document.getElementById('user-input').value = query;
                        this.sendMessage();
                    });
                });

                // –ö–Ω–æ–ø–∫–∞ –Ω–æ–≤–æ—Å—Ç–µ–π
                document.getElementById('news-btn').addEventListener('click', async () => {
                    this.addMessage('–í—ã', '/news');
                    const response = await this.generateResponse('/news');
                    this.addMessage('–¢—Ä–æ–Ω', response);
                });

                // –ö–Ω–æ–ø–∫–∞ –æ—á–∏—Å—Ç–∫–∏
                document.getElementById('clear-btn').addEventListener('click', () => {
                    document.getElementById('chat-messages').innerHTML = `
                        <div class="message tron-message">
                            <strong>–¢—Ä–æ–Ω:</strong> –ß–∞—Ç –æ—á–∏—â–µ–Ω. –ß–µ–º –º–æ–≥—É –ø–æ–º–æ—á—å?
                        </div>
                    `;
                    this.saveToMemory('system', '–ß–∞—Ç –æ—á–∏—â–µ–Ω');
                });

                // –ö–Ω–æ–ø–∫–∞ –≤—ã—Ö–æ–¥–∞
                document.getElementById('logout-btn').addEventListener('click', () => {
                    this.saveToMemory('system', '–°–µ—Å—Å–∏—è –∑–∞–≤–µ—Ä—à–µ–Ω–∞');
                    document.getElementById('main-screen').classList.add('hidden');
                    document.getElementById('login-screen').classList.remove('hidden');
                    document.getElementById('password').value = '';
                    this.isActive = false;
                });
            }

            // –û—Ç–ø—Ä–∞–≤–∫–∞ —Å–æ–æ–±—â–µ–Ω–∏—è
            async sendMessage() {
                const input = document.getElementById('user-input');
                const message = input.value.trim();
                
                if (!message) return;
                
                // –î–æ–±–∞–≤–ª—è–µ–º —Å–æ–æ–±—â–µ–Ω–∏–µ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
                this.addMessage('–í—ã', message);
                this.saveToMemory('user', message);
                
                // –û—á–∏—â–∞–µ–º –ø–æ–ª–µ –≤–≤–æ–¥–∞
                input.value = '';
                
                // –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä "–ø–µ—á–∞—Ç–∞–µ—Ç"
                this.showTypingIndicator();
                
                // –ñ–¥–µ–º –Ω–µ–º–Ω–æ–≥–æ –¥–ª—è —Ä–µ–∞–ª–∏—Å—Ç–∏—á–Ω–æ—Å—Ç–∏
                await new Promise(resolve => setTimeout(resolve, 1000 + Math.random() * 1000));
                
                // –ì–µ–Ω–µ—Ä–∏—Ä—É–µ–º –æ—Ç–≤–µ—Ç
                const response = await this.generateResponse(message);
                
                // –£–±–∏—Ä–∞–µ–º –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä –∏ –¥–æ–±–∞–≤–ª—è–µ–º –æ—Ç–≤–µ—Ç
                this.hideTypingIndicator();
                this.addMessage('–¢—Ä–æ–Ω', response);
                this.saveToMemory('tron', response);
            }

            // –î–æ–±–∞–≤–ª–µ–Ω–∏–µ —Å–æ–æ–±—â–µ–Ω–∏—è –≤ —á–∞—Ç
            addMessage(sender, text) {
                const chatMessages = document.getElementById('chat-messages');
                const messageDiv = document.createElement('div');
                
                messageDiv.className = `message ${sender === '–í—ã' ? 'user-message' : 'tron-message'}`;
                messageDiv.innerHTML = `<strong>${sender}:</strong> ${this.formatMessage(text)}`;
                
                chatMessages.appendChild(messageDiv);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }

            // –§–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏–µ —Å–æ–æ–±—â–µ–Ω–∏—è (–ø–µ—Ä–µ–Ω–æ—Å—ã —Å—Ç—Ä–æ–∫)
            formatMessage(text) {
                return text.replace(/\n/g, '<br>');
            }

            // –ü–æ–∫–∞–∑–∞—Ç—å –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä "–ø–µ—á–∞—Ç–∞–µ—Ç"
            showTypingIndicator() {
                const chatMessages = document.getElementById('chat-messages');
                const indicator = document.createElement('div');
                indicator.id = 'typing-indicator';
                indicator.className = 'message tron-message';
                indicator.innerHTML = '<strong>–¢—Ä–æ–Ω:</strong> <span class="pulse">‚ñã</span>';
                
                chatMessages.appendChild(indicator);
                chatMessages.scrollTop = chatMessages.scrollHeight;
            }

            // –£–±—Ä–∞—Ç—å –∏–Ω–¥–∏–∫–∞—Ç–æ—Ä
            hideTypingIndicator() {
                const indicator = document.getElementById('typing-indicator');
                if (indicator) {
                    indicator.remove();
                }
            }
        }

